        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight">700</item>
        <!--        <item name="android:fontFamily">@font/inter_font</item>-->
        <item name="android:fontFamily">@font/lato_bold</item>
    </style>

    <style name="YouMatterTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/neutral1</item>
        <item name="android:textSize">@dimen/_13sdp</item>
<!--        <item name="android:textStyle">bold</item>-->
        <item name="android:textFontWeight">700</item>
        <!--        <item name="android:fontFamily">@font/inter_font</item>-->
