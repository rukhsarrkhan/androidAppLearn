<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="NewApi">

    <!-- Base core.application theme. -->
    <!--    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">-->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/background_color</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:dialogTheme">@style/DialogTheme</item>
        <item name="android:datePickerStyle">@style/DatePickerSpinnerStyle</item>
        <item name="actionMenuTextColor">@color/selector_text_color</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/lato_font</item>
        <item name="android:itemTextAppearance">@style/CustomMenuTextAppearance</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <item name="android:autofilledHighlight">@android:color/transparent</item>
        // Material
        <item name="textAppearanceButton">@style/TextAppearance.MyApp.Button</item>
        <item name="android:windowAnimationStyle">@style/CustomActivityAnimation</item>
        <item name="android:forceDarkAllowed">false</item>
    </style>

    <style name="ImageCropperTheme" parent="Base.Theme.AppCompat">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:colorBackground">@android:color/background_light</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="toolbarStyle">@style/ImageCropperToolbarTheme</item>
    </style>

    <style name="ImageCropperToolbarTheme" parent="Widget.AppCompat.Toolbar">
        <item name="titleTextColor">@color/white</item>
        <item name="backgroundColor">@color/colorPrimary</item>
    </style>

    <style name="FilePickerTheme" parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:colorBackground">@android:color/background_light</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="toolbarStyle">@style/FilePickerToolbarTheme</item>
        <item name="tabStyle">@style/FilePickerPickerTabLayout</item>
    </style>

    <style name="FilePickerToolbarTheme" parent="Widget.MaterialComponents.Toolbar.Primary">
        <item name="titleTextColor">@color/white</item>
    </style>

    <style name="FilePickerPickerTabLayout" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabTextAppearance">@style/TabLayout</item>
        <item name="tabBackground">@color/colorPrimary</item>
        <item name="tabSelectedTextColor">@color/white</item>
        <item name="tabTextColor">@color/textViewColor</item>
        <item name="tabIndicatorColor">@color/colorPrimary</item>
        <item name="tabGravity">center</item>
        <item name="tabMaxWidth">0dp</item>
    </style>

    <style name="TabLayout">
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/lato_font</item>
        <!--        <item name="android:paddingStart">@dimen/_8sdp</item>
                <item name="android:paddingEnd">@dimen/_8sdp</item>-->
    </style>

    <style name="DrawerArrowStyle" parent="@style/Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@color/colorPrimary</item>
    </style>

    <style name="Widget.App.TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.TextInputLayout</item>
        <item name="shapeAppearance">@style/ShapeAppearance.MaterialComponents.SmallComponent</item>
        <item name="hintTextColor">?attr/colorOnSurface</item>
    </style>

    <style name="ThemeOverlay.App.TextInputLayout" parent="">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorOnSurface">@color/colorPrimary</item>
        <item name="colorError">@color/vivant_red</item>
        <item name="drawableTint">@color/colorPrimary</item>
        <item name="hintTextColor">@color/hlmt_warm_grey</item>
        <item name="editTextStyle">@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox
        </item>
    </style>

    <style name="CustomActivityAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_right</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_out_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_in_left</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_right</item>
    </style>

    <style name="TextAppearance.MyApp.Button" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="CustomMenuTextAppearance">
        <item name="actionMenuTextColor">@color/selector_text_color</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="ToolbarColoredBackArrow" parent="AppTheme">
        <item name="android:textColor">@color/textViewColor</item>
        <item name="android:textColorSecondary">@color/textViewColor</item>
        <!--        <item name="fontPath">fonts/dax_bold.ttf</item>-->
    </style>

    <style name="CustomTextRegular">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/textViewColor</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:background">@drawable/edt_bg_selector</item>
        <item name="android:fontFamily">@font/lato_regular</item>
    </style>

    <style name="CustomTextview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/textViewColor</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:fontFamily">@font/lato_font</item>
    </style>

    <style name="CustomTextviewBold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/textViewColor</item>
        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:fontFamily">@font/lato_bold</item>
    </style>

    <!--NEW STYLES-->

    <style name="ToolbarTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/almost_black</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight">700</item>
        <item name="android:fontFamily">@font/lato_bold</item>
        <item name="android:layout_marginStart">@dimen/_25sdp</item>
        <item name="android:layout_marginEnd">@dimen/_25sdp</item>
    </style>

    <style name="ToolbarTitleStart">
        <item name="android:layout_width">match_parent</item>
