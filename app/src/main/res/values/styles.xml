        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:fontFamily">@font/lato_font</item>
        <item name="android:layout_marginEnd">@dimen/_2sdp</item>
    </style>

    <style name="MyTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <!-- border color -->
        <item name="boxStrokeColor">@color/colorPrimary</item>
        <item name="hintEnabled">false</item>
        <!-- The color of the label when it is collapsed and the text field is active -->
        <item name="hintTextColor">@color/colorPrimary</item>
